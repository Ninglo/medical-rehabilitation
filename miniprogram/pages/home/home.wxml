<navigationBar id="navigationBar" arrow="{{menuArrow}}" bind:arrowClick="getData" 
 placeholderBg="" color="{{color}}" title="{{className}}" back="{{true}}" 
 background="{{barBg}}" fixed="{{fixed}}"  backConfirm="{{backConfirm}}"  
 backStyle="{{backStyle}}"  backEvent="{{backEvent}}" bind:back="backEvent"  
 backHomeEvent="{{backHomeEvent}}" bind:backHome="backHomeEvent">
</navigationBar>

<dropdownmenu districtOpen="{{showIt}}" dropDownMenuTitle="{{dropDownMenuTitle}}"
 dropDownMenuDistrictData="{{classData}}" dropDownMenuSourceData="{{data2}}" 
 dropDownMenuStyleData="{{data3}}" dropDownMenuFilterData="{{data4}}"
 bind:selectedItem="selectedItem">
</dropdownmenu>

